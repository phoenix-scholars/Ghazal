{  
  "manifest_version": 2,
  "name": "ghazal",
  "version": "0.1.0",
  "description": "Ghazal main application",
  "license": "MIT",
  "homepage": "http://dpq.co.ir",
  "icons": {
    "svg": "img/logo.svg",
    "16": "img/logo-16.png",
    "32": "img/logo-32.png",
    "64": "img/logo-64.png",
    "128": "img/logo-128.png"
  },
  "authors": [
    "mostafa barmshory <mostafa.barmshory@dpq.co.ir>"
  ],
  "keywords": [
    "Jayab",
    "Client",
    "Web"
  ],
  "view" : "/main.html",
  "appcache" : "yes",
  "src" : [
  	"module.js",
  	"route.js",
  	"controller/main.js",
  	"controller/application.js",
  	"controller/footer.js",
  	"controller/header.js",
  	"controller/welcome.js"
  ],
  "resource" : [
  	"main.html",
  	"css/style.css",
  	"view/footer.html",
  	"view/header.html",
  	"view/welcome.html",
  	"dialog/login.html"
  ],
  "link" : [{
  		"type":"text/css",
  		"rel":"stylesheet",
  		"media":"screen,projection",
  		"href":"/ghazal/css/style.css"
  	}],
  "meta" : [{
  		"name" : "author",
  		"content" : "Phoinex Scholars Co. (http://dpq.co.ir)"
  	},{
  		"http-equiv" : "Content-Type",
  		"content" : "text/html; charset=UTF-8"
  	},{
  		"name" : "viewport",
  		"content" : "width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no"
  	}],
  "dependencies": {
    "angular": "~1.4.0",
    "angular-animate": "~1.4.5",
    "angular-aria" : "~1.4.0",
    "angular-material" : "~0.11.2",
    "angular-material-css" : "~0.11.2",
    "angular-touch": "~1.4.0",
    "angular-translate": "~2.7.2",
    "angular-ui-router": "~0.2.15",
    "angular-xeditable": "~0.1.9",
    "markdown": "~0.5.0",
    "angular-pluf":"~0.1.0",
    "angular-pluf-saas":"~0.1.0",
    "angular-pluf-help":"~0.1.0",
    "ng-appcache": "~2.0.1",
    "material-design-icons" : "~2.0.0"
  }
}
